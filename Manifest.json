{
    "manifest_version": 3,
    "name": "MOOVE BRAV",
    "version": "1.0",
    "description": "Automatically moves input hubs into selected group.",
    "permissions": ["activeTab", "storage"],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ],
    "icons": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    },
    "action": {
      "default_popup": "popup.html"
    }
}
  